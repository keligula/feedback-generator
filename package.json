    "localtunnel": "^1.9.2",
    "lodash": "^4.17.21",
    "mongoose": "^5.11.7",
    "nodemon": "^1.19.4",
    "npm": "^6.14.9",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^4.2.0",
    "sendgrid": "^5.2.3",
    "stripe": "^6.36.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^2.7.0",
